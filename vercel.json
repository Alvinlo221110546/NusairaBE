{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/*": {
      "cors": {
        "allowMethods": ["GET", "POST", "OPTIONS"],
        "allowOrigins": ["https://nusaira.vercel.app"]  
      }
    }
  }
}
